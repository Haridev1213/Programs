package InterviewQuestions;

public class ArrayEvenNumbers {

public static void main(String[] args)
{
	int arr[]= {15,3,5,8,7,6,4};
	int posi,newposi,temp;
	int even[]=new int[7];
	for(posi=0;posi<7;posi++)
	{
		if(arr[posi]%2==0)
		{
		temp=arr[posi];
		for(int i=0;i<8;i++)
		{	
		System.out.println("even number in new array"+even[temp]);
		}
		}
	}
}

}
